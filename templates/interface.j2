auto {{ item.interface }}
iface {{ item.interface }} inet static
    address {{ item.ipv4 }}
    netmask {{ item.netmask }}
    gateway {{ item.gateway }}
{% if item.ssid is defined %}
    wpa-ssid {{ item.ssid }}
    wpa-psk {{ item.psk }}
{% endif %}
